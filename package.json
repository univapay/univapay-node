{
  "name": "univapay-node",
  "version": "3.1.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:univapay/univapay-node.git"
  },
  "bugs": "https://github.com/univapay/univapay-node/issues",
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "jsnext:main": "dist/esm",
  "keywords": [
    "sdk",
    "univapay",
    "util",
    "module"
  ],
  "devDependencies": {
    "@types/chai": "4.3.5",
    "@types/chai-arrays": "2.0.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/fetch-mock": "7.3.5",
    "@types/jsonwebtoken": "9.0.2",
    "@types/mocha": "10.0.1",
    "@types/node": "20.2.5",
    "@types/sinon": "10.0.15",
    "@types/sinon-chai": "3.2.9",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.7",
    "c8": "^7.14.0",
    "chai": "4.3.7",
    "chai-arrays": "2.2.0",
    "chai-as-promised": "7.1.1",
    "cross-fetch": "3.1.5",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "fetch-mock": "9.11.0",
    "husky": "8.0.3",
    "isomorphic-form-data": "2.0.0",
    "jsonwebtoken": "9.0.0",
    "lint-staged": "13.2.2",
    "mocha": "10.2.0",
    "np": "7.7.0",
    "p-map": "^6.0.0",
    "path-to-regexp": "6.2.1",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "sinon": "15.1.0",
    "sinon-chai": "3.7.0",
    "ts-node": "10.9.1",
    "typedoc": "0.24.7",
    "typedoc-plugin-internal-external": "2.2.0",
    "typescript": "5.0.4",
    "uuid": "9.0.0"
  },
  "dependencies": {
    "@apimatic/json-bigint": "^1.2.0",
    "arrify": "2.0.1",
    "bowser": "^2.11.0",
    "browser-or-node": "2.1.1",
    "change-case": "^4.1.2",
    "events": "^3.3.0",
    "flat": "^5.0.2",
    "is-buffer": "^2.0.5",
    "jwt-decode": "^3.1.2",
    "p-timeout": "^4.1.0",
    "query-string": "^5.1.1",
    "tslib": "^2.5.2"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "format": "prettier \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" --write",
    "test": "c8 mocha",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "preversion": "npm test",
    "docs": "typedoc --options typedoc.json",
    "release": "np --yolo --no-yarn --no-publish",
    "prepare": "husky install"
  }
}

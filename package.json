{
  "name": "univapay-node",
  "version": "1.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:univapay/univapay-node.git"
  },
  "bugs": "https://github.com/univapay/univapay-node/issues",
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "jsnext:main": "dist/esm",
  "keywords": [
    "sdk",
    "univapay",
    "util",
    "module"
  ],
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/chai-arrays": "2.0.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/fetch-mock": "7.3.5",
    "@types/jsonwebtoken": "8.5.8",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.31",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.8",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "c8": "^7.11.2",
    "chai": "4.3.6",
    "chai-arrays": "2.2.0",
    "chai-as-promised": "7.1.1",
    "cross-fetch": "3.1.5",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fetch-mock": "9.11.0",
    "husky": "7.0.4",
    "isomorphic-form-data": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "12.4.1",
    "mocha": "9.2.2",
    "np": "7.6.1",
    "path-to-regexp": "6.2.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "sinon": "13.0.2",
    "sinon-chai": "3.7.0",
    "ts-node": "10.7.0",
    "typedoc": "0.22.15",
    "typedoc-plugin-internal-external": "2.2.0",
    "typescript": "4.6.4",
    "uuid": "8.3.2"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "events": "^3.3.0",
    "is-buffer": "^2.0.5",
    "json-bigint": "^1.0.0",
    "jwt-decode": "^3.1.2",
    "query-string": "^5.1.1",
    "tslib": "^2.4.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "format": "prettier \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" --write",
    "test": "c8 mocha",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:cjs && npm run build:esm",
    "preversion": "npm test",
    "docs": "typedoc --options typedoc.json",
    "release": "np --yolo --no-yarn --no-publish",
    "prepare": "husky install"
  }
}

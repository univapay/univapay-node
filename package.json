{
  "name": "univapay-node",
  "version": "0.4.25",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:univapay/univapay-node.git"
  },
  "bugs": "https://github.com/univapay/univapay-node/issues",
  "engines": {
    "node": "^10 || ^12 || ^14"
  },
  "sideEffects": false,
  "main": "dist/cjs",
  "module": "dist/esm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./": {
      "require": "./dist/cjs/",
      "default": "./dist/esm/"
    }
  },
  "files": [
    "dist"
  ],
  "jsnext:main": "dist/esm",
  "keywords": [
    "sdk",
    "univapay",
    "util",
    "module"
  ],
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/chai-arrays": "1.0.3",
    "@types/chai-as-promised": "7.1.3",
    "@types/fetch-mock": "7.3.3",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.14",
    "@types/node-fetch": "2.5.7",
    "@types/sinon": "9.0.9",
    "@types/sinon-chai": "3.2.5",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "chai": "4.2.0",
    "chai-arrays": "2.2.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.1.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fetch-mock": "9.11.0",
    "mocha": "8.2.1",
    "node-fetch": "2.6.1",
    "np": "7.0.0",
    "nyc": "15.1.0",
    "path-to-regexp": "6.2.0",
    "prettier": "2.2.1",
    "sinon": "9.2.2",
    "sinon-chai": "3.5.0",
    "ts-node": "9.1.1",
    "typedoc": "0.19.2",
    "typedoc-plugin-external-module-name": "4.0.3",
    "typedoc-plugin-internal-external": "2.2.0",
    "typescript": "4.1.3",
    "uuid": "8.3.2"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "events": "^3.2.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-form-data": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "query-string": "^5.1.1",
    "tslib": "^2.1.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "format": "prettier \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" --write",
    "test": "nyc mocha test/specs/*.ts test/specs/**/*.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build:cjs": "tsc",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "preversion": "npm test",
    "docs": "typedoc --options typedoc.json",
    "copy": "cp -rf package.json CHANGELOG.md LICENSE README.md dist",
    "release": "np --no-yarn --no-publish --no-release-draft"
  }
}

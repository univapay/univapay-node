{
  "name": "univapay-node",
  "version": "4.0.39",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:univapay/univapay-node.git"
  },
  "bugs": "https://github.com/univapay/univapay-node/issues",
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "jsnext:main": "dist/esm",
  "keywords": [
    "sdk",
    "univapay",
    "util",
    "module"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/chai-arrays": "^2.0.3",
    "@types/chai-as-promised": "^7.1.8",
    "@types/fetch-mock": "^7.3.8",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.0",
    "@types/sinon-chai": "^3.2.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.3",
    "isomorphic-form-data": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.0.2",
    "mocha": "^10.2.0",
    "np": "^8.0.4",
    "p-map": "^6.0.0",
    "path-to-regexp": "^6.2.1",
    "prettier": "^3.0.3",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "dependencies": {
    "@apimatic/json-bigint": "^1.2.0",
    "arrify": "^2.0.1",
    "bowser": "^2.11.0",
    "browser-or-node": "^2.1.1",
    "change-case": "^4.1.2",
    "events": "^3.3.0",
    "flat": "^5.0.2",
    "is-buffer": "^2.0.5",
    "jwt-decode": "^3.1.2",
    "p-timeout": "^4.1.0",
    "query-string": "^5.1.1",
    "tslib": "^2.6.2"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "format": "prettier \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" --write",
    "test": "c8 mocha",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "preversion": "npm test",
    "docs": "typedoc --options typedoc.json",
    "release": "np --yolo --no-yarn --no-publish",
    "prepare": "husky install"
  }
}

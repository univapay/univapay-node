{
    "name": "univapay-node",
    "version": "1.0.13",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:univapay/univapay-node.git"
    },
    "bugs": "https://github.com/univapay/univapay-node/issues",
    "engines": {
        "node": "^12 || ^14"
    },
    "sideEffects": false,
    "type": "module",
    "main": "dist/cjs",
    "module": "dist/esm",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "default": "./dist/esm/index.js"
        },
        "./*": {
            "require": "./dist/cjs/*.js",
            "default": "./dist/esm/*.js"
        }
    },
    "files": [
        "dist"
    ],
    "jsnext:main": "dist/esm",
    "keywords": [
        "sdk",
        "univapay",
        "util",
        "module"
    ],
    "devDependencies": {
        "@types/chai": "4.2.19",
        "@types/chai-arrays": "2.0.0",
        "@types/chai-as-promised": "7.1.4",
        "@types/fetch-mock": "7.3.3",
        "@types/jsonwebtoken": "8.5.2",
        "@types/mocha": "8.2.2",
        "@types/node": "15.12.5",
        "@types/node-fetch": "2.5.10",
        "@types/sinon": "10.0.2",
        "@types/sinon-chai": "3.2.5",
        "@types/uuid": "8.3.0",
        "@typescript-eslint/eslint-plugin": "4.28.0",
        "@typescript-eslint/parser": "4.28.0",
        "chai": "4.3.4",
        "chai-arrays": "2.2.0",
        "chai-as-promised": "7.1.1",
        "coveralls": "3.1.0",
        "cross-fetch": "3.1.4",
        "eslint": "7.29.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "fetch-mock": "9.11.0",
        "husky": "6.0.0",
        "isomorphic-form-data": "2.0.0",
        "jsonwebtoken": "8.5.1",
        "lint-staged": "11.0.0",
        "mocha": "9.0.1",
        "node-fetch": "2.6.1",
        "np": "7.5.0",
        "nyc": "15.1.0",
        "path-to-regexp": "6.2.0",
        "prettier": "2.3.2",
        "pretty-quick": "3.1.1",
        "sinon": "11.1.1",
        "sinon-chai": "3.7.0",
        "ts-node": "9.1.1",
        "typedoc": "0.21.2",
        "typedoc-plugin-internal-external": "2.2.0",
        "typescript": "4.3.4",
        "uuid": "8.3.2"
    },
    "dependencies": {
        "change-case": "^4.1.2",
        "events": "^3.3.0",
        "is-buffer": "^2.0.5",
        "jwt-decode": "^3.1.2",
        "query-string": "^5.1.1",
        "tslib": "^2.3.0"
    },
    "scripts": {
        "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
        "format": "prettier \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" --write",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "build:cjs": "tsc",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build": "npm run build:cjs && npm run build:esm",
        "preversion": "npm test",
        "docs": "typedoc --options typedoc.json",
        "copy": "cp -rf package.json CHANGELOG.md LICENSE README.md dist",
        "release": "np --no-yarn --no-publish --no-release-draft",
        "prepare": "husky install"
    }
}
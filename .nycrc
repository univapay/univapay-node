{
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register/type-check"
  ],
  "exclude": [
    "coverage/**",
    "test/**",
    "**/*.d.ts"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcovonly"
  ],
  "all": true,
  "check-coverage": false,
  "per-file": false,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 0
}

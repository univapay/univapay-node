{
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "exclude": [
    "src/src/",
    "src/",
    "coverage/**",
    "**/*.d.ts"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html"
  ],
  "cwd": "src",
  "all": true,
  "check-coverage": false,
  "per-file": false,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 0
}

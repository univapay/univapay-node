{
  "extension": [
    ".ts"
  ],
  "exclude": [
    "coverage/**",
    "test/**",
    "**/*.d.ts",
    "docs"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcovonly"
  ],
  "all": true,
  "check-coverage": false,
  "per-file": false,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 0
}
